<krpano>

	<style name="material_ui_text" type="text" css="color:#FFFFFF;text-align:center;font-size:20px;" bg="false" txtshadow="0 1 4 0x000000 1.0" enabled="false" />
	
	<style name="material_ui_text_small" type="text" css="color:#FFFFFF;text-align:center;font-size:16px;" bg="false" txtshadow="0 1 4 0x000000 1.0" enabled="false" />
	
	<layer name="material_ui_text_meson" keep="true" style="material_ui_text" align="center" x="-20%" y="+35%" html="MESON" alpha="0" devices="desktop"/>
	<layer name="material_ui_text_madera" keep="true" style="material_ui_text" align="center" x="+20%" y="+35%" html="MADERA" alpha="0" devices="desktop"/>
	
	<layer name="material_ui_text_meson" keep="true" style="material_ui_text_small" align="center" x="-10%" y="+30%" html="MESON" alpha="0" devices="handheld"/>
	<layer name="material_ui_text_madera" keep="true" style="material_ui_text_small" align="center" x="+10%" y="+30%" html="MADERA" alpha="0" devices="handheld"/>
	<action name="change_meson" scope="local" args="mesoncolor">
		
		copy(curr_name,xml.scene);
		if(get(curr_name) == 'scene_2_cocina2',
			tour_loadscene(calc('scene_4_madera_neutra_meson_' + mesoncolor));
			,
			indexoftxt(i,get(curr_name),'meson');
			add(i,6);
			tour_loadscene(calc(slice(curr_name,0,i) + mesoncolor));
		);
	</action>
	
	<action name="change_madera" scope="local" args="maderacolor">
		copy(curr_name,xml.scene);
		if(get(curr_name) == 'scene_2_cocina2',
			tour_loadscene(calc('scene_4_madera_' + maderacolor +  '_meson_gris'));
			,
			indexoftxt(i,get(curr_name),'_meson');
			tour_loadscene(calc('scene_4_madera_' + maderacolor + slice(curr_name,i,curr_name.length)));
		);
	</action>
	
	<action name="set_madera_ui" scope="local" args="nmadera, onoff">
		calc(floorname_ui,'maderaui' + nmadera);
		copy(floorname,layer[get(floorname_ui)].url);
		indexoftxt(i,get(floorname),'madera');
		add(i,7);
		calc(floorurlbase,slice(floorname,0,i));
		calc(layer[get(floorname_ui)].url,floorurlbase+onoff+'.png');
	</action>
	
	<action name="set_meson_ui" scope="local" args="nmeson, onoff">
		calc(floorname_ui,'mesonui' + nmeson);
		copy(floorname,layer[get(floorname_ui)].url);
		indexoftxt(i,get(floorname),'meson');
		add(i,6);
		calc(floorurlbase,slice(floorname,0,i));
		calc(layer[get(floorname_ui)].url,floorurlbase+onoff+'.png');
	</action>
	
	<action name="disable_material_ui" scope="local">
		
		
		tween(layer[material_ui_text_meson].alpha,0);
		tween(layer[material_ui_text_madera].alpha,0);
		
		set_madera_ui(1,off); set_madera_ui(2,off); set_madera_ui(3,off);
		set_meson_ui(1,off); set_meson_ui(2,off); set_meson_ui(3,off);
		set(layer[mesonui1].enabled,false); tween(layer[mesonui1].alpha,0);
		set(layer[mesonui2].enabled,false); tween(layer[mesonui2].alpha,0);
		set(layer[mesonui3].enabled,false); tween(layer[mesonui3].alpha,0);
		set(layer[maderaui1].enabled,false); tween(layer[maderaui1].alpha,0);
		set(layer[maderaui2].enabled,false); tween(layer[maderaui2].alpha,0);
		set(layer[maderaui3].enabled,false); tween(layer[maderaui3].alpha,0);
	</action>
	
	<action name="enable_material_ui" scope="local">
		
		tween(layer[material_ui_text_meson].alpha,1);
		tween(layer[material_ui_text_madera].alpha,1);
		set(layer[mesonui1].enabled,true); tween(layer[mesonui1].alpha,1);
		set(layer[mesonui2].enabled,true); tween(layer[mesonui2].alpha,1);
		set(layer[mesonui3].enabled,true); tween(layer[mesonui3].alpha,1);
		set(layer[maderaui1].enabled,true); tween(layer[maderaui1].alpha,1);
		set(layer[maderaui2].enabled,true); tween(layer[maderaui2].alpha,1);
		set(layer[maderaui3].enabled,true); tween(layer[maderaui3].alpha,1);
	</action>
	
