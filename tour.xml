			indexoftxt(i,get(curr_name),'_meson');
			loadscene(calc('scene_4_madera_' + maderacolor + slice(curr_name,i,curr_name.length)),null,MERGE|KEEPVIEW|KEEPMOVING,BLEND(0.5));
		);
	</action>
	
