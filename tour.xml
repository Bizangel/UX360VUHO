	<action name="setintervalflash" scope="local">
	calc(clearid,'flashid'+caller.name);
			setinterval(clearid,1.1,
				if (caller !== null,
					
					if(caller.flashingout == true,
						tween(caller.borderalpha,0,1);
						,
						tween(caller.borderalpha,.6,1);
					);
					
					if(caller.borderalpha GE .6, 
						set(caller.flashingout,true);
					);
					
					if(caller.borderalpha LE 0, 
						set(caller.flashingout,false);
					);
				);
				,
				<!-- null caller -->
				clearinterval(clearid);
			);
	</action>
	<action name="clearintervalflash" scope="local">
		clearinterval(calc('flashid' + caller.name));
	</action>
	<action name="borderalphatween1" scope="local">
		tween(hotspot[get(caller.name)].borderalpha,1);
	</action>
	
	<action name="borderalphatween0" scope="local">
		tween(hotspot[get(caller.name)].borderalpha,0);
	</action>
	
