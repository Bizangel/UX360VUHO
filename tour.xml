	<events webvr_onentervr="reload_current_scene();" webvr_onexitvr="reload_current_scene();" />
	
	<action name="reload_current_scene"> 
		copy(scenename,xml.scene);
		indexoftxt(isOutside,get(scenename),'exterior');
		if(isOutside != -1,
			
			loadscene(get(scenename),null,MERGE|KEEPVIEW|KEEPMOVING,BLEND(0.5));
		);
		<!-- Loads scene, conserving floor color and mueble color -->
		
		indexoftxt(i,get(scenename),'_');
		calc(i2,lastindexof(get(scenename),'_'));
		
		calc(scene_n,slice(scenename,i+1,i2));
		
		if(scene_n LT 5,
			calc(to_load,'scene_' + scene_n + '_' + layer[variableHolder].curr_floor_color + '-' +layer[variableHolder].curr_mueble_color);
			,
			calc(to_load,'scene_' + scene_n + '_' + layer[variableHolder].curr_floor_color);
		);
		
		loadscene(get(to_load),null,MERGE|KEEPVIEW|KEEPMOVING,BLEND(0.5));
		
	</action>
	
	<!-- Overload Tour Loading so it keeps coordinates and has nice animation 
	ALSO UPDATED FLAG SO IT BLENDS AND KEEPS VIEW-->
	<action name="skin_loadscene" scope="local" args="newscenenameorindex, blendmode">
		if(webvr.isenabled AND scene.count GT 1,
			set(hotspot[skin_webvr_prev_scene].visible, false);
			set(hotspot[skin_webvr_next_scene].visible, false);
		);
		
		calc(layer[skin_thumbborder].parent, 'skin_thumb_' + scene[get(newscenenameorindex)].index);
		layer[skin_thumbs].scrolltocenter(get(scene[get(newscenenameorindex)].thumbx), get(scene[get(newscenenameorindex)].thumby));
		loadscene(get(scene[get(newscenenameorindex)].name), null, get(skin_settings.loadscene_flags), get(blendmode));
		tween(view.tx|view.ty|view.tz, calc(image.ox + '|' + image.oy + '|' + image.oz), 2.0, default);
		
		
	</action>
	
	<action name="generate_floorspots" autorun="onstart" scope="local">
		set(floorheight, 160.0);
		for(set(i,0), i LT style.count, inc(i),
			if (style[get(i)].autohotspot == true,
				copy(st, style[get(i)]);
				addhotspot(calc('hotspot_' + (i+1)), hs);
				hs.loadstyle(floorspot);
				copy(hs.tx, st.ox);
				copy(hs.tz, st.oz);
				calc(hs.ty, floorheight + 1.0*st.oy - 1.0);
				copy(hs.linkedscene, st.linkedscene);
			);
		);
	</action>
	
