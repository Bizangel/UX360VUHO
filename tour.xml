	<action name="change_meson" scope="local" args="mesoncolor">
		
		copy(curr_name,xml.scene);
		if(get(curr_name) == 'scene_2_cocina2',
			loadscene(calc('scene_4_madera_neutra_meson_' + mesoncolor),null,MERGE|KEEPVIEW|KEEPMOVING,BLEND(0.5));
			,
			indexoftxt(i,get(curr_name),'meson');
			add(i,6);
			loadscene(calc(slice(curr_name,0,i) + mesoncolor),null,MERGE|KEEPVIEW|KEEPMOVING,BLEND(0.5));
		);
	</action>
	
	<action name="change_madera" scope="local" args="maderacolor">
		copy(curr_name,xml.scene);
		if(get(curr_name) == 'scene_2_cocina2',
			loadscene(calc('scene_4_madera_' + maderacolor +  '_meson_gris'),null,MERGE|KEEPVIEW|KEEPMOVING,BLEND(0.5));
